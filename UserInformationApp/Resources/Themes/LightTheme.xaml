<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:UserInformationApp.Resources.Themes">
    <!-- Defined Border Brushes -->
    <SolidColorBrush x:Key="Static.Border" Color="Black"/>

    <!-- BusyIndicator Brushes-->
    <SolidColorBrush x:Key="BusyIndicator.Static.Background" Color="#FF98abb7"/>
    <SolidColorBrush x:Key="BusyIndicatorProgressBar.Static.Background" Color="#FF98abb7"/>
